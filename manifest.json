{
  "manifest_version": 2,
  "name": "New Tab Word List",
  "version": "1.0.0",
  "description": "Random common English words with Uzbek translation",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "chrome_url_overrides": {"newtab": "start-page.html"}
}
